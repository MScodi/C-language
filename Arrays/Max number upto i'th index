//Max number upto i'th index

#include <algorithm>
#include <iostream>
using namespace std;
//function defination
int max_(int a[], int n){
    int max=0;
    for(int i=0; i<n ; i++){
        if(a[i]>max)
            max=a[i];
    cout<<max<<" ";    
    }
}
//main function
int main()
{
    int arr[] = { 12, 3, 5, 7, 19 };
    int n = sizeof(arr) / sizeof(arr[0]), k = 2;
    max_(arr, n);
    return 0;
}
